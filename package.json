    "build:dev": "cross-env NODE_ENV=development react-scripts build -- --profile",
    "test": "react-scripts test",
    "test:ci": "cross-env NODE_ENV=test JEST_SUITE_NAME=\"dpio-asset-editor\" JEST_JUNIT_SUITE_NAME=\"{filename}\" JEST_JUNIT_CLASSNAME=\"{classname}\" JEST_JUNIT_TITLE=\"{classname} > {title}\" JEST_JUNIT_ANCESTOR_SEPARATOR=\" â€º \" react-scripts test --all --ci --useStderr --watchAll=false --env=jsdom --reporters=default --reporters=jest-junit --coverage --coverageReporters=text --coverageReporters=cobertura",
