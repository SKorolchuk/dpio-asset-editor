    "compile": "tsc --version && tsc --project tsconfig.json --noEmit --noErrorTruncation --pretty",
    "fix": "npm run lint-fix && npm run prettier-fix",
    "prettier": "prettier --list-different \"*.{html,ts,tsx,js,jsx,json,css,scss,md}\" \"{docs,src,public}/*.{html,ts,tsx,js,jsx,json,css,scss,md}\" \"src/**/*.{html,ts,tsx,js,jsx,json,css,scss,md}\"",
    "prettier-fix": "prettier --write \"*.{html,ts,tsx,js,jsx,json,css,scss,md}\" \"{docs,src,public}/*.{html,ts,tsx,js,jsx,json,css,scss,md}\" \"src/**/*.{html,ts,tsx,js,jsx,json,css,scss,md}\"",
    "lint": "eslint --version && node --max_old_space_size=2048 ./node_modules/eslint/bin/eslint --config .eslintrc.js",
    "lint-fix": "eslint --version && node --max_old_space_size=2048 ./node_modules/eslint/bin/eslint --fix --config .eslintrc.js",
    "lint-sass": "sass-lint --version && sass-lint 'src/*.scss' 'src/**/*.scss' -v -q",
    "precommit": "npm run compile && npm run lint && npm run lint-sass && npm run prettier && npm run test -- --all --watchAll=false"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  }
